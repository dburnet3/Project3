{
  "name": "mern",
  "version": "1.0.0",
  "description": "Mern Demo",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.3",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "core-util-is": "^1.0.2",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mysql2": "^1.6.5",
    "node-sass": "^4.12.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pretty-checkbox": "^3.0.3",
    "pretty-checkbox-react": "^1.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-horizontal-scrolling-menu": "^0.7.3",
    "react-moment": "^0.9.2",
    "react-router-dom": "^5.0.1",
    "react-scrollable-anchor": "^0.6.1",
    "react-selectable-fast": "^3.0.0",
    "reactstrap": "^8.0.1",
    "semantic-ui-react": "^0.88.0",
    "sequelize": "^5.13.1",
    "validator": "^11.1.0"
  }
}
